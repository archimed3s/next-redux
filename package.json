{
  "name": "dp-dashboard",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "description": "DP Dashboard",
  "main": "src/server/index.ts",
  "scripts": {
    "prestart": "npm i",
    "start": "npm run cover && npm run doc && npm run build",
    "build": "next build src/client && tsc --project tsconfig.server.json",
    "prod": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=production node dist/server",
    "serve": "nodemon",
    "lint": "tslint -c tslint.json -p tsconfig.json && stylelint './src/**/*.tsx' --config stylelint.config.js",
    "test": "jest",
    "testdev": "jest --watchAll",
    "cover": "jest --coverage",
    "doc": "typedoc"
  },
  "author": "Heorhii Kuzmenko from Pupil team",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virtuocityvr/dp-dashboard.git"
  },
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/virtuocityvr/dp-dashboard/issues"
  },
  "homepage": "https://github.com/virtuocityvr/dp-dashboard#readme",
  "dependencies": {
    "@zeit/next-typescript": "^1.1.1",
    "auth0-js": "^9.10.4",
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "js-cookie": "^2.2.0",
    "jwks-rsa": "^1.5.1",
    "next": "^8.1.0",
    "next-fonts": "^0.17.0",
    "next-optimized-images": "^2.5.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "request-promise-native": "^1.0.7",
    "styled-components": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@types/auth0-js": "^9.10.2",
    "@types/cookie-parser": "^1.4.1",
    "@types/enzyme": "^3.9.3",
    "@types/express": "^4.16.1",
    "@types/express-jwt": "0.0.42",
    "@types/jest": "^24.0.13",
    "@types/js-cookie": "^2.2.2",
    "@types/next": "^8.0.5",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/request-promise-native": "^1.0.16",
    "@types/styled-components": "^4.1.15",
    "babel-jest": "^24.8.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "husky": "^2.3.0",
    "image-trace-loader": "^0.1.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.0.0",
    "imagemin-svgo": "^7.0.0",
    "jest": "^24.8.0",
    "jest-styled-components": "^6.3.1",
    "jimp": "^0.6.4",
    "lqip-loader": "^2.2.0",
    "nodemon": "^1.19.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.8.6",
    "responsive-loader": "^1.2.0",
    "stylelint": "^10.0.1",
